let triangle = "#";
for(let counter = 0; counter<=7;counter++){
    console.log(triangle);
    triangle += "#";
}

// FizzBuzz
for(let count = 0; count <= 100; count++){
    if(count % 3 == 0 && count % 5 == 0 && count != 0){
        console.log("FizzBuzz");
    } else if(count % 3 == 0 && !count % 5 == 0){ 
        console.log("Fizz");
    } else if(count % 5 == 0 && !count % 3 == 0){
        console.log("Buzz"); 
    } else 
    console.log(count);
  

}
